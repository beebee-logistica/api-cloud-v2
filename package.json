{
  "name": "api-cloud-v2",
  "version": "1.0.0",
  "description": "API para upgrade na cloud",
  "main": "bin/www",
  "scripts": {
    "prebuild": "npm run static",
    "build": "tsc -w -p .",
    "directories": "find server -type d | sed \"s|^server/|build/|\" | xargs mkdir -p",
    "prestatic": "npm run directories",
    "static": "echo \"Copiando arquivos est√°ticos\" && find server -type f ! -name \"*.ts\" | sed \"s|^server/||\" | xargs \"-I{}\" cp \"server/{}\" \"build/{}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prodest/api-cloud-v2.git"
  },
